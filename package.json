{
	"name": "my-tree",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "yarn run rm && tsc -b && vite build",
		"lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
		"preview": "vite preview",
		"preview:build": "yarn run check && yarn run build && vite preview",
		"stylelint": "stylelint \"src/**/*.{css,scss}\"",
		"stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
		"format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path ./.prettierignore",
		"check": "yarn run stylelint:fix && yarn run lint && yarn run format",
		"prepare": "husky install",
		"commit": "yarn run check && cz",
		"rm": "rimraf ./dist",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"lint-staged": {
		"*.{css,scss}": "yarn run stylelint:fix",
		"*.{js,jsx,ts,tsx}": "yarn run lint",
		"*.{js,jsx,ts,tsx,json,css,scss,md}": "yarn run format"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.2.6",
		"clsx": "^2.1.1",
		"normalize.css": "^8.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.24.1",
		"react-tooltip": "^5.27.1",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "1.6.1",
		"@eslint/compat": "^1.1.0",
		"@storybook/addon-essentials": "^8.1.11",
		"@storybook/addon-interactions": "^8.1.11",
		"@storybook/addon-links": "^8.1.11",
		"@storybook/addon-onboarding": "^8.1.11",
		"@storybook/blocks": "^8.1.11",
		"@storybook/react": "^8.1.11",
		"@storybook/react-vite": "^8.1.11",
		"@storybook/test": "^8.1.11",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.19",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "8.x",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.9.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.3",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.7",
		"eslint-plugin-storybook": "^0.8.0",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"postcss": "^8.4.39",
		"prettier": "^3.3.2",
		"rimraf": "^5.0.8",
		"sass": "^1.77.6",
		"storybook": "^8.1.11",
		"stylelint": "^16.6.1",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-config-standard-scss": "^13.1.0",
		"stylelint-order": "^6.0.4",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.2.2",
		"vite": "^5.3.1"
	},
	"eslintConfig": {
		"extends": [
			"plugin:storybook/recommended"
		]
	}
}
